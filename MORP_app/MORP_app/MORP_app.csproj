<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>src\dark_icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="src\close.png" />
    <None Remove="src\dark_icon.png" />
    <None Remove="src\logo.png" />
    <None Remove="src\maxi.png" />
    <None Remove="src\mini.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="src\dark_icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.3124.44" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="src\close.png" />
    <Resource Include="src\dark_icon.png" />
    <Resource Include="src\logo.png" />
    <Resource Include="src\maxi.png" />
    <Resource Include="src\mini.png" />
  </ItemGroup>

</Project>
